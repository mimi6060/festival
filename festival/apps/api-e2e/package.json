{
  "name": "@festival/festival-api-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "api-e2e",
    "implicitDependencies": ["api"],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": ["{projectRoot}/test-output/jest/coverage"],
        "options": {
          "jestConfig": "apps/api-e2e/jest.config.cts",
          "passWithNoTests": false,
          "runInBand": true
        },
        "dependsOn": ["api:build"]
      },
      "e2e:watch": {
        "executor": "@nx/jest:jest",
        "options": {
          "jestConfig": "apps/api-e2e/jest.config.cts",
          "watch": true
        }
      },
      "e2e:coverage": {
        "executor": "@nx/jest:jest",
        "options": {
          "jestConfig": "apps/api-e2e/jest.config.cts",
          "coverage": true
        }
      }
    }
  },
  "dependencies": {
    "axios": "^1.6.0",
    "@nestjs/testing": "^11.0.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@prisma/client": "^6.0.0",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@types/supertest": "^6.0.0",
    "jest": "^29.7.0",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.0"
  }
}
