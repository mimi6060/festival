<!-- Divider -->
<table border="0" cellpadding="0" cellspacing="0" width="100%">
  <tr>
    <td style="padding: {{#if paddingTop}}{{paddingTop}}{{else}}20{{/if}}px 0 {{#if paddingBottom}}{{paddingBottom}}{{else}}20{{/if}}px 0;">
      <table border="0" cellpadding="0" cellspacing="0" width="100%">
        <tr>
          <td style="border-top: 1px solid {{#if color}}{{color}}{{else}}#eaeaea{{/if}};"></td>
        </tr>
      </table>
    </td>
  </tr>
</table>
