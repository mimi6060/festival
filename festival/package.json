  "scripts": {
    "start": "node dist/api/main.js",
    "start:prod": "NODE_ENV=production node dist/api/main.js",
    "build": "nx build api --prod",
    "build:api": "nx build api --prod",
    "serve:api": "nx serve api",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:reset": "prisma migrate reset",
    "db:reset": "prisma migrate reset --force && npm run prisma:seed",
    "db:seed:demo": "SEED_DEMO_MODE=true ts-node prisma/seed.ts",
    "test:e2e": "nx e2e api-e2e",
    "test:e2e:watch": "nx run api-e2e:e2e:watch",
    "test:e2e:coverage": "nx run api-e2e:e2e:coverage",
    "test:e2e:ci": "nx run api-e2e:e2e:ci",
    "test:e2e:auth": "nx e2e api-e2e --testPathPattern=auth.spec.ts",
    "test:e2e:festivals": "nx e2e api-e2e --testPathPattern=festivals.spec.ts",
    "test:e2e:tickets": "nx e2e api-e2e --testPathPattern=tickets.spec.ts",
    "test:e2e:cashless": "nx e2e api-e2e --testPathPattern=cashless.spec.ts",
    "test:api": "nx test api",
    "test:api:watch": "nx test api --configuration=watch",
    "test:api:cov": "nx test:cov api",
    "test:api:ci": "nx test api --configuration=ci",
    "vercel:build": "nx build web --prod",
    "vercel:dev": "nx serve web",
    "vercel:build:api": "nx build api --prod",
    "vercel:build:all": "nx run-many --target=build --projects=api,web --prod",
    "docs:generate": "ts-node scripts/generate-openapi.ts",
    "docs:serve": "nx serve api && echo 'Swagger UI available at http://localhost:3000/docs'"
  },
